<DoctorsList>:
    BoxLayout:
        orientation: 'vertical'
        pos_hint: {'center_y': 0.7}
        MDScreen:
            MDBoxLayout:
                orientation: 'vertical'
                spacing: dp(10)
                padding: dp(16)
            MDLabel:
                text: 'Doctors'
                theme_text_color: 'Primary'
                halign: 'center'
                font_style: 'H4'
                pos_hint: {'center_y': 0.7}
            ScrollView:
                MDBoxLayout:
                    orientation: 'vertical'
                    spacing: dp(10)
                    padding: dp(16)

                    OneLineListItem:
                        text: 'William Osler         2pm-6pm'
                        on_release: 
                            app.app_book('William Osler', '2pm-6pm')
                            app.show_popup()

                    OneLineListItem:
                        text: 'Gerbrand Bakker   10am-4pm'
                        on_release:
                            app.app_book('Gerbrand Bakker', '10am-4pm')
                            app.show_popup()

                    OneLineListItem:
                        text: 'Charles Best          12am-5pm'
                        on_release:
                            app.app_book('Charles Best', '12am-5pm')
                            app.show_popup()
                        
                    OneLineListItem:
                        text: 'Elizabeth                1pm-4pm'
                        on_release:
                            app.app_book('Elizabeth', '1pm-4pm')
                            app.show_popup()
                        
                    OneLineListItem:
                        text: 'James Carson      2pm-4pm'
                        on_release: 
                            app.app_book('James Carson', '2pm-4pm')
                            app.show_popup()
            MDBoxLayout:
                MDFloatingActionButton:
                    icon: 'img/back.png'
                    pos_hint: {'center_y': 0.75}
                    on_release: app.change_screen('home_screen')
                        
